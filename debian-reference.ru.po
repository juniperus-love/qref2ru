# type: Content of: <book><chapter><section><section><screen>
#. type: Content of: <book><chapter><section><section><screen>
#: debian-reference.en.xmlt:4039
#, no-wrap
msgid ""
"$ mkdir junk; cd junk; touch 1.txt 2.txt 3.c 4.h .5.txt ..6.txt\n"
"$ echo *.txt\n"
"1.txt 2.txt\n"
"$ echo *\n"
"1.txt 2.txt 3.c 4.h\n"
"$ echo *.[hc]\n"
"3.c 4.h\n"
"$ echo .*\n"
". .. .5.txt ..6.txt\n"
"$ echo .*[^.]*\n"
".5.txt ..6.txt\n"
"$ echo [^1-3]*\n"
"4.h\n"
"$ cd ..; rm -rf junk"
msgstr ""
"$ mkdir junk; cd junk; touch 1.txt 2.txt 3.c 4.h .5.txt ..6.txt\n"
"$ echo *.txt\n"
"1.txt 2.txt\n"
"$ echo *\n"
"1.txt 2.txt 3.c 4.h\n"
"$ echo *.[hc]\n"
"3.c 4.h\n"
"$ echo .*\n"
". .. .5.txt ..6.txt\n"
"$ echo .*[^.]*\n"
".5.txt ..6.txt\n"
"$ echo [^1-3]*\n"
"4.h\n"
"$ cd ..; rm -rf junk"

