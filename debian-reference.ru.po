"    |   |-- branches\n"
"    |   |-- tags\n"
"    |   |   |-- release-1.0\n"
"    |   |   `-- release-2.0\n"
"    |   |\n"
"    |   `-- trunk\n"
"    |       |-- file1\n"
"    |       |-- file2\n"
"    |       `-- file3\n"
"    |\n"
"    `-- module2"

